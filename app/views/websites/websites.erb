    <p>Welcome, <%= current_user.username.capitalize %>!</p>
    <form action="/websites/new" method="GET">
      <button type="submit" name="button">Add New Website</button>
    </form>
    <br>

    <% @websites.each.with_index do |website, index| %>
      <div>
        <a target="_blank" href="<%= website.content %>" class="button"><%= website.content %></a> <a href="/tags/<%= website.tag.id %>"><sub><i><%= website.tag.content %></i></sub></a>
        <label>- <a href="/websites/<%= website.id %>/edit">Edit</a></label>
      </div>
      <br>
    <% end %>
    <br><br>
    <a href="/logout">Logout</a>
